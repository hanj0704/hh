            <cl:output std:sid="output-88c60ef5" id="opt15" value="theme/assets/less/controls/fileinput/STYLE_CONTROLS_FILEINPUT_01.less">
              <cl:verticaldata std:sid="v-data-7eea5e7b" width="100px" height="25px" autosize="height"/>
            </cl:output>
            <cl:verticallayout std:sid="v-layout-3d91933b" leftmargin="30" rightmargin="30" topmargin="20" bottommargin="20"/>
          </cl:group>
          <cl:output std:sid="output-c66842e8" value="데이트 인풋에 &quot;single-datepicker&quot; 클래스를 적용합니다. 전역적인 데이트 인풋 컨트롤에 사용하는 경우 루트 스타일러의 class에 클래스를 적용하지 않도록 합니다.&#xD;&#xA;&#xD;&#xA;파일 인풋에 &quot;file-browser&quot; 클래스를 적용합니다. 우측에 표시되는 &quot;Browse&quot;라는 텍스트는 가상 요소의 content를 통해 표시되는 텍스트이므로 유동적으로 변경하기 어려울 수 있습니다.">
            <cl:verticaldata std:sid="v-data-d3939fb0" width="1040px" height="126px" autosize="height"/>
          </cl:output>
          <cl:udc std:sid="ud-control-b7a23aae" id="ace1" type="udc.cmn.Ace">
            <cl:verticaldata std:sid="v-data-ae2cf1f3" width="300px" height="50px" autosize="height"/>
            <cl:property name="value" value=".cl-fileinput.file-browser {&#xD;&#xA;&#x9;background-image: none; // 기본 그라데이션 스타일 제거&#xD;&#xA;&#x9;background-color: #ffffff;&#xD;&#xA;&#x9;border: 1px solid #babfc7;&#xD;&#xA;&#x9;border-radius: 3px;&#xD;&#xA;&#x9;color: #333333;&#xD;&#xA;&#x9;text-align: left;&#xD;&#xA;&#x9;transition: color .15s ease-in-out, background-color .15s ease-in-out, background-image .15s ease-in-out, border-color .15s ease-in-out;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 인풋 플레이스홀더 스타일 */&#x9;&#xD;&#xA;&#x9;.cl-text {&#xD;&#xA;&#x9;&#x9;padding: 0px 75px 0px 5px;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 인풋 플레이스홀더 스타일 */&#x9;&#xD;&#xA;&#x9;&#x9;&amp;.cl-placeholder {&#xD;&#xA;&#x9;&#x9;&#x9;color: #cfd2d8 !important;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 인풋 포커스 스타일 */&#x9;&#xD;&#xA;&#x9;&amp;:focus {&#xD;&#xA;&#x9;&#x9;border-color: #1E9FF2;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&amp;:after {&#xD;&#xA;&#x9;&#x9;&#x9;content: &quot;Browse&quot;;&#xD;&#xA;&#x9;&#x9;&#x9;border-color: #1E9FF2;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 브라우저 스타일 */&#xD;&#xA;&#x9;&amp;:after {&#xD;&#xA;&#x9;&#x9;content: &quot;Browse&quot;;&#xD;&#xA;&#x9;&#x9;position: absolute;&#xD;&#xA;&#x9;&#x9;display: flex;&#xD;&#xA;&#x9;&#x9;align-items: center;&#xD;&#xA;&#x9;&#x9;justify-content: center;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;right: 1px;&#xD;&#xA;&#x9;&#x9;top: 1px;&#xD;&#xA;&#x9;&#x9;bottom: 1px;&#xD;&#xA;&#x9;&#x9;width: 70px;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;background-color: #f1f5fa;&#xD;&#xA;&#x9;&#x9;border-left: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;border-top-right-radius: 3px;&#xD;&#xA;&#x9;&#x9;border-bottom-right-radius: 3px;&#xD;&#xA;&#x9;&#x9;color: #333333;&#xD;&#xA;&#x9;&#x9;line-height: 24px;&#xD;&#xA;&#x9;&#x9;text-align: center;&#xD;&#xA;&#x9;&#x9;transition: color .15s ease-in-out, background-color .15s ease-in-out, background-image .15s ease-in-out, border-color .15s ease-in-out;&#xD;&#xA;&#x9;}&#xD;&#xA;}" type="string"/>
            <cl:property name="language" value="ace/mode/less" type="enumeration"/>
          </cl:udc>
          <cl:verticallayout std:sid="v-layout-81987a1e" spacing="15" leftmargin="0" rightmargin="0" topmargin="20" bottommargin="40"/>
        </cl:group>
