            <cl:verticallayout std:sid="v-layout-3d91933b" leftmargin="30" rightmargin="30" topmargin="20" bottommargin="20"/>
          </cl:group>
          <cl:output std:sid="output-c66842e8" value="그리드 컨트롤의 각 행의 상태 값에 따라 행 스타일을 변경하고 싶은 경우 다음과 같이 스타일을 작성하세요.&#xD;&#xA;&#xD;&#xA;행에 대한 스타일 색상을 지정할 때 헥스 코드(#000000 과 같은 형태)로 색상을 지정하는 것보다 rgba() 형태로 색상을 지정하도록 권고하고 있으므로 행에 대한 스타일은 반드시 rgba() 형태를 사용하십시오. 헥스 코드 또는 rgb() 형식으로 색상을 지정하는 경우 병합된 셀에서 일부 텍스트가 가려지는 현상이 발생할 수 있습니다.&#xD;&#xA;&#xD;&#xA;만약 전역적으로 그리드 행에 대한 스타일을 변경해야 하는 경우에는 grid.part.less 파일에서 다음의 스타일을 적용할 수 있도록 합니다.">
            <cl:verticaldata std:sid="v-data-d3939fb0" width="1040px" height="126px" autosize="height"/>
          </cl:output>
          <cl:udc std:sid="ud-control-b7a23aae" id="ace1" type="udc.cmn.Ace">
            <cl:verticaldata std:sid="v-data-ae2cf1f3" width="300px" height="50px" autosize="height"/>
            <cl:property name="value" value=".cl-grid.table-status {&#xD;&#xA;&#x9;/* 그리드 디테일 영역 */&#xD;&#xA;&#x9;.cl-grid-detail {&#xD;&#xA;&#x9;&#x9;/* 그리드 로우 영역 */&#xD;&#xA;&#x9;&#x9;.cl-grid-row {&#xD;&#xA;&#x9;&#x9;&#x9;/* 보기 모드의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-viewing {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 홀수 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-odd-row {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 짝수 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-even-row {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 추가된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-inserted {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(30, 159, 242, 0.15);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 수정된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-modified {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(255, 193, 7, 0.18);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 삭제된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-deleted {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(0, 0, 0, .05);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 추가 후 삭제된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-nullified {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(0, 0, 0, .03);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 선택된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-selected {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(255, 235, 59, 0.45);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 편집 모드인 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;.cl-editing {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: rgba(255, 235, 59, 0.65);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 호버된 행의 로우 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&amp;:hover {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;}" type="string"/>
            <cl:property name="language" value="ace/mode/less" type="enumeration"/>
          </cl:udc>
          <cl:verticallayout std:sid="v-layout-81987a1e" spacing="15" leftmargin="0" rightmargin="0" topmargin="20" bottommargin="40"/>
        </cl:group>
        <cl:group std:sid="group-f5df0ee5" class="card-inner">
