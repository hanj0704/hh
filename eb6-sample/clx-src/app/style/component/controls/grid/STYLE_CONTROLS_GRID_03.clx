            <cl:verticaldata std:sid="v-data-ae2cf1f3" width="300px" height="50px" autosize="height"/>
            <cl:property name="value" value=".cl-treecell.treecell-levels {&#xD;&#xA;&#x9;.cl-treecell-item {&#xD;&#xA;&#x9;&#x9;color: #333333;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 트리 셀 포커스 스타일 */&#xD;&#xA;&#x9;&#x9;&amp;:focus {&#xD;&#xA;&#x9;&#x9;&#x9;outline: none; // 포커스 링 삭제&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 트리 셀 1레벨 스타일 */&#xD;&#xA;&#x9;&#x9;&amp;.tree-item-level-1 {&#xD;&#xA;&#x9;&#x9;&#x9;background-color: rgba(244, 67, 54, 0.15);&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 트리 셀 2레벨 스타일 */&#xD;&#xA;&#x9;&#x9;&amp;.tree-item-level-2 {&#xD;&#xA;&#x9;&#x9;&#x9;background-color: rgba(49, 203, 114, 0.15);&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 트리 셀 3레벨 스타일 */&#xD;&#xA;&#x9;&#x9;&amp;.tree-item-level-3 {&#xD;&#xA;&#x9;&#x9;&#x9;background-color: rgba(33, 150, 243, 0.15);&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;}" type="string"/>
            <cl:property name="language" value="ace/mode/less" type="enumeration"/>
