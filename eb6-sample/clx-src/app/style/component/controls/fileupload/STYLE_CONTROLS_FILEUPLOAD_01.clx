          </cl:udc>
          <cl:group std:sid="group-6a755066" id="grp16" class="ref-card info">
            <cl:verticaldata std:sid="v-data-a577cd31" width="1040px" height="96px" autosize="height"/>
            <cl:output std:sid="output-d6122608" id="opt14" class="ref-title" value="경로 : LESS 파일">
              <cl:verticaldata std:sid="v-data-157990b2" width="100px" height="25px"/>
            </cl:output>
            <cl:output std:sid="output-88c60ef5" id="opt15" value="theme/assets/less/controls/fileupload/STYLE_CONTROLS_FILEUPLOAD_01.less">
              <cl:verticaldata std:sid="v-data-7eea5e7b" width="100px" height="25px" autosize="height"/>
            </cl:output>
            <cl:verticallayout std:sid="v-layout-3d91933b" leftmargin="30" rightmargin="30" topmargin="20" bottommargin="20"/>
          </cl:group>
          <cl:output std:sid="output-c66842e8" value="파일 업로드 컨트롤의 class에 &quot;file-uploader&quot; 클래스를 작성합니다.&#xD;&#xA;&#xD;&#xA;파일 업로드 컨트롤 내 컬럼 크기는 파일 업로드의 속성을 통해 수정하도록 제공되고 있지 않으므로 스타일 작성을 통해 width 크기를 결정해야 합니다. 필요 시 다음 소스 코드의 주석을 참고하여 헤더와 디테일에 해당하는 체크박스 컬럼, 파일 이름 컬럼, 파일 크기 컬럼의 크기를 수정하십시오.&#xD;&#xA;&#xD;&#xA;파일 업로드에 다양한 형식의 파일을 추가할 때 각 파일에 해당하는 아이콘은 .cl-filetype-*** 형태로 퍼블리싱 포인트가 제공됩니다. 예를 들어 *.png 파일을 파일로 추가했을 때 파일 아이콘에 해당하는 퍼블리싱 포인트가 .cl-filetype-png 라는 이름으로 제공됩니다.&#xD;&#xA;&#xD;&#xA;전역적인 파일 업로드 컨트롤에 사용하는 경우 루트 스타일러의 class에 클래스를 적용하지 않도록 합니다. LESS 작성 시에도 전역적인 컨트롤에 적용될 수 있는 형태로 작성되어야 합니다. 따라서 전역적으로 컨트롤의 스타일을 변경하고 싶은 경우 file-upload.part.less에서 다음의 스타일을 적용하면 됩니다.">
            <cl:verticaldata std:sid="v-data-d3939fb0" width="1040px" height="126px" autosize="height"/>
          </cl:output>
          <cl:udc std:sid="ud-control-b7a23aae" id="ace1" type="udc.cmn.Ace">
            <cl:verticaldata std:sid="v-data-ae2cf1f3" width="300px" height="50px" autosize="height"/>
            <cl:property name="value" value=".cl-fileupload.file-uploader {&#xD;&#xA;&#x9;background-color: #ffffff; &#xD;&#xA;&#x9;border: 1px solid #babfc7;&#xD;&#xA;&#x9;border-radius: 5px;&#xD;&#xA;&#x9;color: #333333;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 내 텍스트 스타일 (공통) */&#xD;&#xA;&#x9;.cl-text {&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 헤더 스타일 */&#xD;&#xA;&#x9;.cl-fileupload-header {&#xD;&#xA;&#x9;&#x9;background-color: #edf1f4;&#xD;&#xA;&#x9;&#x9;background-image: none; // 기본 파일 업로드 헤더 그라데이션 제거&#xD;&#xA;&#x9;&#x9;border-bottom: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;color: #2C353E;&#xD;&#xA;&#x9;&#x9;font-weight: bold;&#xD;&#xA;&#x9;&#x9;height: 30px;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 헤더 체크박스 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-checkheader {&#xD;&#xA;&#x9;&#x9;&#x9;border-right: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 헤더 파일 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-nameheader {&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 헤더 파일 크기 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-sizeheader {&#xD;&#xA;&#x9;&#x9;&#x9;border-left: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;&#x9;min-width: 85px; // width로 지정 시 컬럼 크기 리사이즈가 제대로 동작하지 않을 수 있음&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 디테일 영역 스타일 */&#xD;&#xA;&#x9;.cl-fileupload-detail {&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 파일 행 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-file {&#xD;&#xA;&#x9;&#x9;&#x9;height: 28px;&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 파일 행 체크박스 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-checkcolumn {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 파일 행 파일 아이콘 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-fileicon {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-default.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-size: 16px;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-position: right center;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;width: 21px;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;/* 파일 업로드 파일 확장자에 따른 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-jpg, &amp;.cl-filetype-jpeg, &amp;.cl-filetype-png, &amp;.cl-filetype-svg {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-pic.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-avi, &amp;.cl-filetype-mp4, &amp;.cl-filetype-wmv, &amp;.cl-filetype-mov {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-video.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-txt, &amp;.cl-filetype-html, &amp;.cl-filetype-xml, &amp;.cl-filetype-hwp, &amp;.cl-filetype-hwt {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-text.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-doc, &amp;.cl-filetype-docx, &amp;.cl-filetype-dotx, &amp;.cl-filetype-docm {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-word.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-pdf {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-pdf.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-xlsx, &amp;.cl-filetype-xlsm, &amp;.cl-filetype-xltx, &amp;.cl-filetype-xlsb, &amp;.cl-filetype-xlam {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-excel.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-pptx, &amp;.cl-filetype-pptm, &amp;.cl-filetype-ppt, &amp;.cl-filetype-potx, &amp;.cl-filetype-potm, &amp;.cl-filetype-pot {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-ppt.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;.cl-filetype-zip, &amp;.cl-filetype-7z {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/fileupload/icon-filetype-zip.png&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 파일 행 파일 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-filename {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;padding-left: 5px;&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 파일 행 파일 크기 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-sizecolumn {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;padding-right: 5px;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;min-width: 85px;&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드에서 업로드된 파일 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-uploadedfile {&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드에서 업로드된 파일 체크박스 컬럼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-checkcolumn {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드에서 업로드된 파일 다운로드 버튼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-download {&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 푸터 스타일 */&#xD;&#xA;&#x9;.cl-fileupload-footer {&#xD;&#xA;&#x9;&#x9;background-color: #edf1f4;&#xD;&#xA;&#x9;&#x9;border-top: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;color: #333333;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 푸터 텍스트 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-text {&#xD;&#xA;&#x9;&#x9;&#x9;font-size: 13px;&#xD;&#xA;&#x9;&#x9;&#x9;font-weight: normal;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 푸터 버튼 스타일 */&#xD;&#xA;&#x9;&#x9;.cl-fileupload-buttons {&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 푸터 버튼 공통 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-button {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-image: none; // 기본 버튼 그라데이션 제거&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: #ffffff;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;border: 1px solid #babfc7;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;border-radius: 3px;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;color: #333333;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;font-weight: normal;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;padding: 3px 15px;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;text-shadow: none; // 기본 버튼 텍스트 그림자 제거&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;:focus {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;border-color: #babfc7;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;/* 파일 업로드 푸터 보내기 버튼 스타일 */&#xD;&#xA;&#x9;&#x9;&#x9;.cl-fileupload-button.cl-fileupload-send {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;background-color: #28D094;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;border-color: #28D094;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;color: #ffffff;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&amp;:focus {&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;border-color: #28D094;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 체크박스 컬럼 공통 스타일 */&#xD;&#xA;&#x9;.cl-fileupload-checkheader, .cl-fileupload-checkcolumn {&#xD;&#xA;&#x9;&#x9;width: 35px !important;&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;/* 파일 업로드 내 체크박스 공통 스타일 */&#xD;&#xA;&#x9;.cl-fileupload-checkbox {&#xD;&#xA;&#x9;&#x9;background-image: url(&quot;../../../../images/controls/checkbox/icon-checkbox-circle.png&quot;);&#xD;&#xA;&#x9;&#x9;background-size: 16px;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;/* 파일 업로드 내 체크박스 선택 시 스타일 */&#xD;&#xA;&#x9;&#x9;&amp;.cl-chekced {&#xD;&#xA;&#x9;&#x9;&#x9;background-image: url(&quot;../../../../images/controls