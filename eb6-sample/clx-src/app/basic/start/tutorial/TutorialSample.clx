            <cl:property name="value" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xD;&#xA;&#xD;&#xA;&lt;!DOCTYPE mapper&#xD;&#xA;    PUBLIC &quot;-//mybatis.org//DTD Mapper 3.0//EN&quot;&#xD;&#xA;    &quot;http://mybatis.org/dtd/mybatis-3-mapper.dtd&quot;>&#xD;&#xA;&#xD;&#xA;&lt;mapper namespace=&quot;emp&quot;>&#xD;&#xA;&#x9;&lt;select id=&quot;getEmpList&quot; resultType=&quot;HashMap&quot;>&#xD;&#xA;&#x9;&#x9;SELECT EMPLOYEE_ID&#xD;&#xA;&#x9;&#x9;&#x9;   ,FIRST_NAME&#xD;&#xA;&#x9;&#x9;&#x9;   ,LAST_NAME&#xD;&#xA;&#x9;&#x9;&#x9;   ,EMAIL&#xD;&#xA;&#x9;&#x9;&#x9;   ,PHONE_NUMBER&#xD;&#xA;&#x9;&#x9;&#x9;   ,HIRE_DATE&#xD;&#xA;&#x9;&#x9;&#x9;   ,DEPARTMENT_ID&#xD;&#xA;&#x9;&#x9;&#x9;   ,SALARY&#xD;&#xA;&#x9;&#x9;&#x9;   FROM EMP&#xD;&#xA;&#x9;&#x9;&lt;if test=&quot; NAME != '' and NAME != null &quot;>&#xD;&#xA;&#x9;&#x9;&#x9;WHERE LAST_NAME LIKE concat('%', #{NAME}, '%')&#xD;&#xA;&#x9;&#x9;&#x9;OR FIRST_NAME LIKE concat('%', #{NAME}, '%')&#xD;&#xA;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&lt;/select>&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&lt;select id = &quot;getDeptList&quot; resultType=&quot;HashMap&quot;>&#xD;&#xA;&#x9;&#x9;SELECT DEPT_NO&#xD;&#xA;&#x9;&#x9;&#x9;  ,DEPT_NAME&#xD;&#xA;&#x9;&#x9;&#x9;   FROM DEPT;&#xD;&#xA;&#x9;&lt;/select>&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&lt;insert id=&quot;insertEmp&quot; parameterType=&quot;HashMap&quot;>&#xD;&#xA;&#x9;&#x9;INSERT INTO EMP(&#xD;&#xA;&#x9;&#x9;&#x9;  HIRE_DATE&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;FIRST_NAME != null and FIRST_NAME != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, FIRST_NAME&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;LAST_NAME != null and LAST_NAME != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, LAST_NAME&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;EMAIL != null and EMAIL != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, EMAIL&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;PHONE_NUMBER != null and PHONE_NUMBER != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, PHONE_NUMBER&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;DEPARTMENT_ID != null and DEPARTMENT_ID != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, DEPARTMENT_ID&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;SALARY != null and SALARY != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, SALARY&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;)VALUES(&#xD;&#xA;&#x9;&#x9;&#x9;&lt;choose>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;when test=&quot;HIRE_DATE != null and HIRE_DATE != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;to_date(#{HIRE_DATE} , 'YYYYMMDD') &#xD;&#xA;&#x9;&#x9;&#x9;&lt;/when>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;otherwise>&#xD;&#xA;&#x9;&#x9;&#x9;CURRENT_DATE&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/otherwise>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/choose>&#xD;&#xA;&#x9;&#x9;  &#x9;&lt;if test=&quot;FIRST_NAME != null and FIRST_NAME != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{FIRST_NAME}&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;  &#x9;&lt;if test=&quot;LAST_NAME != null and LAST_NAME != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{LAST_NAME}&#xD;&#xA;&#x9;&#x9;  &#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;  &#x9;&lt;if test=&quot;EMAIL != null and EMAIL != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{EMAIL}&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;PHONE_NUMBER != null and PHONE_NUMBER != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{PHONE_NUMBER}&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;DEPARTMENT_ID != null and DEPARTMENT_ID != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{DEPARTMENT_ID}&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;&#x9;&lt;if test=&quot;SALARY != null and SALARY != ''&quot;>&#xD;&#xA;&#x9;&#x9;&#x9;, #{SALARY}&#xD;&#xA;&#x9;&#x9;&#x9;&lt;/if>&#xD;&#xA;&#x9;&#x9;  &#x9;)&#xD;&#xA;&#x9;&lt;/insert>&#xD;&#xA;&#xD;&#xA;&#x9;&lt;update id=&quot;updateEmp&quot;> &#xD;&#xA;&#x9;&#x9;UPDATE EMP SET &#xD;&#xA;&#x9;&#x9;&#x9;  FIRST_NAME = #{FIRST_NAME}&#xD;&#xA;&#x9;&#x9;&#x9;, LAST_NAME = #{LAST_NAME}&#xD;&#xA;&#x9;&#x9;&#x9;, EMAIL = #{EMAIL}&#xD;&#xA;&#x9;&#x9;&#x9;, PHONE_NUMBER = #{PHONE_NUMBER}&#xD;&#xA;&#x9;&#x9;&#x9;, HIRE_DATE = to_date(#{HIRE_DATE} , 'YYYYMMDD') &#xD;&#xA;&#x9;&#x9;&#x9;, DEPARTMENT_ID = #{DEPARTMENT_ID}&#xD;&#xA;&#x9;&#x9;&#x9;, SALARY = #{SALARY}&#xD;&#xA;&#x9;&#x9;WHERE EMPLOYEE_ID = #{EMPLOYEE_ID}&#xD;&#xA;&#x9;&lt;/update>&#xD;&#xA;&#xD;&#xA;&#x9;&lt;delete id=&quot;deleteEmp&quot;>&#xD;&#xA;&#x9;&#x9;DELETE FROM EMP WHERE EMPLOYEE_ID = #{EMPLOYEE_ID}&#xD;&#xA;&#x9;&lt;/delete>&#xD;&#xA;&lt;/mapper>" type="string"/>
