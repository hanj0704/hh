                  <cl:verticaldata std:sid="v-data-e5c97393" width="1100px" height="56px" autosize="height"/>
                </cl:output>
                <cl:udc std:sid="ud-control-72e754af" id="ace7" type="udc.cmn.Ace">
                  <cl:verticaldata std:sid="v-data-51b40cad" width="1100px" height="29px" autosize="height"/>
                  <cl:property name="value" value="/*&#xD;&#xA; * &quot;저장&quot; 버튼(btnSave)에서 click 이벤트 발생 시 호출.&#xD;&#xA; * 사용자가 컨트롤을 클릭할 때 발생하는 이벤트.&#xD;&#xA; */&#xD;&#xA;function onBtnSaveClick(/* cpr.events.CMouseEvent */ e){&#xD;&#xA;&#x9;/** &#xD;&#xA;&#x9; * @type cpr.controls.Button&#xD;&#xA;&#x9; */&#xD;&#xA;&#x9;var btnSave = e.control;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;var vcSubSave = app.lookup(&quot;subSave&quot;);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;vcSubSave.send();&#xD;&#xA;}" type="string"/>
                </cl:udc>
                <cl:verticallayout std:sid="v-layout-a8546207" spacing="0"/>
